{
  "name": "@youam/sdk",
  "version": "0.2.0",
  "type": "module",
  "description": "Universal Agent Messaging (UAM) TypeScript SDK -- end-to-end encrypted, trust-on-first-use agent messaging",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "bin": {
    "uam": "./dist/cli/index.cjs"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/youam/uam.git",
    "directory": "ts-sdk"
  },
  "homepage": "https://youam.network",
  "bugs": {
    "url": "https://github.com/youam/uam/issues"
  },
  "keywords": [
    "uam",
    "universal-agent-messaging",
    "agent",
    "messaging",
    "e2e-encryption",
    "ed25519",
    "tofu",
    "trust-on-first-use",
    "handshake",
    "contact-card",
    "dns-verification",
    "relay",
    "a2a"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "commander": "^12.0.0",
    "libsodium-wrappers": "^0.7.15",
    "uuid": "^10.0.0",
    "viem": "^2.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/ws": "^8.5.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  },
  "license": "MIT"
}
